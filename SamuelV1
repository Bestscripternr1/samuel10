    Increment = 5,
    ValueName = "Speed",
    Callback = function(value)
        speedBoostValue = value
    end
})

-- Apply speed boost
spawn(function()
    while true do
        if speedBoostEnabled and player.Character then
            local hrp = player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                local forward = hrp.CFrame.LookVector
                hrp.Velocity = Vector3.new(forward.X * speedBoostValue, hrp.Velocity.Y, forward.Z * speedBoostValue)
            end
        end
        wait(0.1)
    end
end)

