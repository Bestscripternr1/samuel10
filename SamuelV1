local VehicleTab = Window:MakeTab({ Name = "üöó‚îÇVehicle", Icon = "rbxassetid://3926307971" })

VehicleTab:AddSection({ Name = "üöÄ Vehicle Flight" })

-- Enhanced Vehicle Fly
local vehicleFlyEnabled = false
local vehicleFlySpeed = 100
local vehicleFlyConnection

local function updateVehicleFly()
    if not vehicleFlyEnabled then return end
    
    local character = LocalPlayer.Character
    if not character then return end
    
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then return end
    
    local seat = humanoid.SeatPart
    if not seat or seat.Name ~= "DriveSeat" then return end
    
    local vehicle = seat.Parent
    if not vehicle then return end
    
    if not vehicle.PrimaryPart then
        vehicle.PrimaryPart = seat
    end
    
    -- Movement controls
    local direction = Vector3.zero
    if UserInputService:IsKeyDown(Enum.KeyCode.W) then direction += Camera.CFrame.LookVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.S) then direction -= Camera.CFrame.LookVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.A) then direction -= Camera.CFrame.RightVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.D) then direction += Camera.CFrame.RightVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.E) then direction += Vector3.new(0, 1, 0) end
    if UserInputService:IsKeyDown(Enum.KeyCode.Q) then direction -= Vector3.new(0, 1, 0) end
    
    if direction.Magnitude > 0 then
        direction = direction.Unit * vehicleFlySpeed
        vehicle:PivotTo(vehicle:GetPivot() + direction * 0.05)
        
        -- Smooth flight
        for _, part in ipairs(vehicle:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Velocity = Vector3.new(0, 0, 0)
                part.RotVelocity = Vector3.new(0, 0, 0)
            end
        end
    end
end

VehicleTab:AddToggle({
    Name = "üöÄ Vehicle Fly",
    Default = false,
    Callback = function(v)
        vehicleFlyEnabled = v
        if v then
            vehicleFlyConnection = RunService.RenderStepped:Connect(updateVehicleFly)
            OrionLib:MakeNotification({
                Name = "Vehicle Fly",
                Content = "üöÄ Vehicle Flight Activated!",
                Time = 3
            })
        elseif vehicleFlyConnection then
            vehicleFlyConnection:Disconnect()
            vehicleFlyConnection = nil
        end
    end
})

VehicleTab:AddSlider({
    Name = "‚ö° Flight Speed",
    Min = 10,
    Max = 500,
    Default = 100,
    Color = Color3.fromRGB(0, 255, 100),
    Increment = 10,
    ValueName = "Speed",
    Callback = function(v) vehicleFlySpeed = v end
})

VehicleTab:AddSection({ Name = "üé® Vehicle Customization" })

