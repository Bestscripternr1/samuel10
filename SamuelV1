-- AIMBOT TAB (Enhanced)
-- ===========================
local AimbotTab = Window:MakeTab({
    Name = "üéØ‚îÇAimbot",
    Icon = "rbxassetid://7733855923",
    PremiumOnly = false
})

AimbotTab:AddSection({
    Name = "Aimbot Settings"
})

local aimbotEnabled = false
local aimbotKey = Enum.KeyCode.Q
local aimbotFOV = 50
local aimbotSmoothness = 0.5
local aimbotPart = "Head"

local fovCircle = Drawing.new("Circle")
fovCircle.Visible = false
fovCircle.Radius = aimbotFOV
fovCircle.Color = Color3.fromRGB(255, 255, 255)
fovCircle.Thickness = 2
fovCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)

AimbotTab:AddToggle({
    Name = "üéØ Aimbot",
    Default = false,
    Callback = function(Value)
        aimbotEnabled = Value
        fovCircle.Visible = Value
    end
})

AimbotTab:AddBind({
    Name = "üéØ Aimbot Key",
    Default = Enum.KeyCode.Q,
    Hold = false,
    Callback = function()
        aimbotEnabled = not aimbotEnabled
        fovCircle.Visible = aimbotEnabled
    end
})

AimbotTab:AddSlider({
    Name = "üéØ FOV Size",
    Min = 10,
    Max = 200,
    Default = 50,
    Color = Color3.fromRGB(255, 0, 0),
    Increment = 5,
    ValueName = "Pixels",
    Callback = function(Value)
        aimbotFOV = Value
        fovCircle.Radius = Value
    end
})

AimbotTab:AddSlider({
    Name = "üéØ Smoothness",
    Min = 1,
    Max = 10,
    Default = 5,
    Color = Color3.fromRGB(0, 255, 0),
    Increment = 1,
    ValueName = "Level",
    Callback = function(Value)
        aimbotSmoothness = (11 - Value) / 10
    end
})

AimbotTab:AddDropdown({
    Name = "üéØ Aim Part",
    Default = "Head",
    Options = {"Head", "Torso", "HumanoidRootPart"},
    Callback = function(Value)
        aimbotPart = Value
    end
})

-- Aimbot Logic
RunService.RenderStepped:Connect(function()
    fovCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
    
    if aimbotEnabled and UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) then
        local target = nil
        local closestDistance = aimbotFOV
        
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character then
                local character = player.Character
                local part = character:FindFirstChild(aimbotPart)
                
                if part then
                    local position, onScreen = Camera:WorldToViewportPoint(part.Position)
                    
                    if onScreen then
                        local mousePosition = UserInputService:GetMouseLocation()
                        local distance = (Vector2.new(position.X, position.Y) - mousePosition).Magnitude
                        
                        if distance < closestDistance then
                            closestDistance = distance
                            target = part
                        end
                    end
                end
            end
        end
        
        if target then
            local targetPosition = Camera:WorldToViewportPoint(target.Position)
            local mousePosition = UserInputService:GetMouseLocation()
            
            local delta = (Vector2.new(targetPosition.X, targetPosition.Y) - mousePosition)
            local smoothedDelta = delta * aimbotSmoothness
            
            mousemoverel(smoothedDelta.X, smoothedDelta.Y)
        end
    end
end)

