local carFlyEnabled = false
local carFlightSpeed = 100
local carFlightHeight = 10

local function getPlayerVehicle()
    local vehicles = workspace:FindFirstChild("Vehicles")
    if vehicles then
        return vehicles:FindFirstChild(player.Name)
    end
    return nil
end

Tab2:AddToggle({
    Name = "üü¢ Car Fly",
    Default = false,
    Callback = function(Value)
        carFlyEnabled = Value
        OrionLib:MakeNotification({
            Name = "Car Fly",
            Content = Value and "Aktiviert - W/A/S/D/Space/Shift" or "Deaktiviert",
            Time = 3
        })
    end
})

Tab2:AddSlider({
    Name = "üü¢ Car Fly Speed",
    Min = 10,
    Max = 500,
    Default = 100,
    Color = Color3.fromRGB(0, 255, 0),
    Increment = 10,
    ValueName = "Speed",
    Callback = function(Value)
        carFlightSpeed = Value
    end
})

Tab2:AddSlider({
    Name = "üü¢ Car Fly H√∂he",
    Min = 0,
    Max = 50,
    Default = 10,
    Color = Color3.fromRGB(0, 200, 255),
    Increment = 1,
    ValueName = "Height",
    Callback = function(Value)
        carFlightHeight = Value
    end
})

RunService.RenderStepped:Connect(function()
    if carFlyEnabled then
        local vehicle = getPlayerVehicle()
        local char = player.Character
        if vehicle and char then
            local driveSeat = vehicle:FindFirstChild("DriveSeat")
            if driveSeat and driveSeat.Occupant == char.Humanoid then
                if not vehicle.PrimaryPart then
                    vehicle.PrimaryPart = driveSeat
                end
                
                local moveDirection = Vector3.zero
                local camera = workspace.CurrentCamera
                
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDirection = moveDirection + camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDirection = moveDirection - camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDirection = moveDirection - camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDirection = moveDirection + camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveDirection = moveDirection + Vector3.new(0, 1, 0)
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    moveDirection = moveDirection - Vector3.new(0, 1, 0)
                end
                
                if moveDirection.Magnitude > 0 then
                    moveDirection = moveDirection.Unit * carFlightSpeed
                end
                
                local currentPos = vehicle.PrimaryPart.Position
                local targetY = carFlightHeight
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) or UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    targetY = currentPos.Y + moveDirection.Y
                else
                    targetY = carFlightHeight
                end
                
                vehicle:SetPrimaryPartCFrame(
                    CFrame.new(
                        currentPos + Vector3.new(moveDirection.X, 0, moveDirection.Z) * 0.1,
                        currentPos + camera.CFrame.LookVector * 100
                    ) * CFrame.new(0, targetY - currentPos.Y, 0)
                )
                
                driveSeat.Velocity = Vector3.zero
                driveSeat.RotVelocity = Vector3.zero
            end
        end
    end
end)

