-- Vehicle Mod Updates
spawn(function()
    while true do
        if vehicleGod then
            local vehicle = workspace.Vehicles:FindFirstChild(player.Name)
            if vehicle then
                vehicle:SetAttribute("CurrentHealth", math.huge)
                vehicle:SetAttribute("IsOn", true)
            end
        end
        if infiniteFuel then
            local vehicle = workspace.Vehicles:FindFirstChild(player.Name)
            if vehicle then
                vehicle:SetAttribute("CurrentFuel", math.huge)
            end
        end
        wait(0.5)
    end
end)

