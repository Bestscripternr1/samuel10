-- ESP TAB (Enhanced)
-- ===========================
local ESPTab = Window:MakeTab({
    Name = "üëÅÔ∏è‚îÇESP",
    Icon = "rbxassetid://7733766307",
    PremiumOnly = false
})

ESPTab:AddSection({
    Name = "ESP Settings"
})

local espEnabled = false
local espBoxes = {}
local espNames = {}
local espTracers = {}

local function createESP(player)
    local box = Drawing.new("Square")
    box.Visible = false
    box.Color = Settings.ESPColor
    box.Thickness = 2
    box.Filled = false
    espBoxes[player] = box
    
    local name = Drawing.new("Text")
    name.Visible = false
    name.Color = Settings.ESPColor
    name.Size = 14
    name.Center = true
    name.Outline = true
    espNames[player] = name
    
    local tracer = Drawing.new("Line")
    tracer.Visible = false
    tracer.Color = Settings.ESPColor
    tracer.Thickness = 1
    espTracers[player] = tracer
end

local function updateESP()
    for player, box in pairs(espBoxes) do
        if player and player.Character and espEnabled then
            local character = player.Character
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            
            if humanoidRootPart then
                local position, onScreen = Camera:WorldToViewportPoint(humanoidRootPart.Position)
                
                if onScreen then
                    -- Update box
                    local size = Vector2.new(2000 / position.Z, 3000 / position.Z)
                    box.Size = size
                    box.Position = Vector2.new(position.X - size.X / 2, position.Y - size.Y / 2)
                    box.Visible = true
                    
                    -- Update name
                    espNames[player].Text = player.Name
                    espNames[player].Position = Vector2.new(position.X, position.Y - size.Y / 2 - 20)
                    espNames[player].Visible = true
                    
                    -- Update tracer
                    espTracers[player].From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
                    espTracers[player].To = Vector2.new(position.X, position.Y)
                    espTracers[player].Visible = true
                else
                    box.Visible = false
                    espNames[player].Visible = false
                    espTracers[player].Visible = false
                end
            end
        else
            box.Visible = false
            if espNames[player] then espNames[player].Visible = false end
            if espTracers[player] then espTracers[player].Visible = false end
        end
    end
end

ESPTab:AddToggle({
    Name = "üëÅÔ∏è ESP",
    Default = false,
    Callback = function(Value)
        espEnabled = Value
        if Value then
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer then
                    createESP(player)
                end
            end
            
            RunService.RenderStepped:Connect(updateESP)
        else
            for _, box in pairs(espBoxes) do
                box:Remove()
            end
            for _, name in pairs(espNames) do
                name:Remove()
            end
            for _, tracer in pairs(espTracers) do
                tracer:Remove()
            end
            
            espBoxes = {}
            espNames = {}
            espTracers = {}
        end
    end
})

ESPTab:AddColorpicker({
    Name = "üé® ESP Color",
    Default = Color3.fromRGB(0, 255, 0),
    Callback = function(Value)
        Settings.ESPColor = Value
        for _, box in pairs(espBoxes) do
            box.Color = Value
        end
        for _, name in pairs(espNames) do
            name.Color = Value
        end
        for _, tracer in pairs(espTracers) do
            tracer.Color = Value
        end
    end
})

-- Player Added/Removed handlers
Players.PlayerAdded:Connect(function(player)
    if espEnabled then
        createESP(player)
    end
end)

Players.PlayerRemoving:Connect(function(player)
    if espBoxes[player] then
        espBoxes[player]:Remove()
        espBoxes[player] = nil
    end
    if espNames[player] then
        espNames[player]:Remove()
        espNames[player] = nil
    end
    if espTracers[player] then
        espTracers[player]:Remove()
        espTracers[player] = nil
    end
end)

