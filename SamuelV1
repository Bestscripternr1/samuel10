local autoReviveEnabled = false
ReviveTab:AddToggle({
    Name = "üíâ Auto-Revive",
    Default = false,
    Flag = "AutoRevive",
    Callback = function(v)
        autoReviveEnabled = v
        if v then
            spawn(function()
                while autoReviveEnabled do
                    if player.Character then
                        local humanoid = player.Character:FindFirstChild("Humanoid")
                        if humanoid and humanoid.Health < 20 then
                            MakeNotification("Auto-Revive", "Low health detected - Healing at hospital...", "rbxassetid://7072723219")
                            wait(5)
                            if humanoid then
                                humanoid.Health = humanoid.MaxHealth
                                MakeNotification("Auto-Revive", "Fully healed!", "rbxassetid://7072723219")
                            end
                        end
                    end
                    wait(1)
                end
            end)
        end
        MakeNotification("Auto-Revive", v and "Enabled" or "Disabled", "rbxassetid://4483345998")
    end
})

