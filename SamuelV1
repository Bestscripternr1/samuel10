-- Noclip System
local noclipEnabled = false
local noclipConnection = nil

Tab1:AddToggle({
    Name = "‚ö´ Noclip",
    Default = false,
    Callback = function(v)
        noclipEnabled = v
        
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
        
        if v then
            noclipConnection = RunService.Stepped:Connect(function()
                if player.Character and noclipEnabled then
                    for _, part in pairs(player.Character:GetDescendants()) do
                        if part:IsA("BasePart") and part.CanCollide then
                            part.CanCollide = false
                        end
                    end
                end
            end)
            
            OrionLib:MakeNotification({
                Name = "Noclip",
                Content = "Enabled",
                Time = 3
            })
        else
            if player.Character then
                for _, part in pairs(player.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
            OrionLib:MakeNotification({
                Name = "Noclip",
                Content = "Disabled",
                Time = 3
            })
        end
    end
})

-- Player Fly System (FIXED)
local flying = false
local flightSpeed = 50
local flightVelocity = Vector3.new(0, 0, 0)
local flightConnection = nil

local function updateFlight()
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
        return
    end
    
    local root = player.Character.HumanoidRootPart
    local camera = workspace.CurrentCamera
    
    -- Reset velocity
    flightVelocity = Vector3.new(0, 0, 0)
    
    -- Movement inputs
    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
        flightVelocity = flightVelocity + camera.CFrame.LookVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
        flightVelocity = flightVelocity - camera.CFrame.LookVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
        flightVelocity = flightVelocity - camera.CFrame.RightVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
        flightVelocity = flightVelocity + camera.CFrame.RightVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
        flightVelocity = flightVelocity + Vector3.new(0, 1, 0)
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
        flightVelocity = flightVelocity - Vector3.new(0, 1, 0)
    end
    
    -- Normalize and apply speed
    if flightVelocity.Magnitude > 0 then
        flightVelocity = flightVelocity.Unit * flightSpeed
    end
    
    -- Apply velocity
    root.Velocity = flightVelocity
    
    -- Keep character upright
    root.CFrame = CFrame.new(root.Position, root.Position + camera.CFrame.LookVector)
end

Tab1:AddToggle({
    Name = "‚ö´ Player Fly",
    Default = false,
    Callback = function(v)
        flying = v
        
        if flightConnection then
            flightConnection:Disconnect()
            flightConnection = nil
        end
        
        if v then
            -- Check if in vehicle
            local character = player.Character
            if character and character:FindFirstChild("Humanoid") then
                local humanoid = character.Humanoid
                if humanoid.SeatPart then
                    OrionLib:MakeNotification({
                        Name = "Fly Error",
                        Content = "Cannot fly while in vehicle!",
                        Time = 3
                    })
                    return
                end
            end
            
            flightConnection = RunService.Heartbeat:Connect(updateFlight)
            
            OrionLib:MakeNotification({
                Name = "Player Fly",
                Content = "Enabled - Use WASD, Space, Shift",
                Time = 3
            })
        else
            -- Reset velocity when turning off
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
            end
            
            OrionLib:MakeNotification({
                Name = "Player Fly",
                Content = "Disabled",
                Time = 3
            })
        end
    end
})

Tab1:AddSlider({
    Name = "‚ö´ Fly Speed",
    Min = 1,
    Max = 200,
    Default = 50,
    Color = Color3.fromRGB(69,64,64),
    Increment = 1,
    ValueName = "Speed",
    Callback = function(Value)
        flightSpeed = Value
    end    
})

-- Anti-Fall/Damage
local antiFallEnabled = false
local antiFallConnection = nil

Tab1:AddToggle({
    Name = "‚ö´ Anti-Fall/Damage",
    Default = false,
    Callback = function(Value)
        antiFallEnabled = Value
        
        if antiFallConnection then
            antiFallConnection:Disconnect()
            antiFallConnection = nil
        end
        
        if Value then
            antiFallConnection = RunService.Heartbeat:Connect(function()
                if player.Character and antiFallEnabled then
                    local humanoid = player.Character:FindFirstChild("Humanoid")
                    local rootPart = player.Character:FindFirstChild("HumanoidRootPart")
                    
                    if humanoid and rootPart then
                        -- Prevent fall damage
                        if humanoid:GetState() == Enum.HumanoidStateType.Freefall then
                            local velocity = rootPart.Velocity
                            if velocity.Y < 0 then
                                rootPart.Velocity = Vector3.new(velocity.X, math.max(velocity.Y, -25), velocity.Z)
                            end
                        end
                        
                        -- Check if falling from high
                        if rootPart.Velocity.Y < -50 then
                            rootPart.Velocity = Vector3.new(rootPart.Velocity.X, -25, rootPart.Velocity.Z)
                        end
                    end
                end
            end)
            
            OrionLib:MakeNotification({
                Name = "Anti-Fall",
                Content = "Enabled",
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "Anti-Fall",
                Content = "Disabled",
                Time = 3
            })
        end
    end
})

-- Anti-Taser System
local AntiTaserEnabled = false
local antiTaserConnections = {}

local function disconnectAntiTaser()
    for _, connection in ipairs(antiTaserConnections) do
        if connection then
            pcall(function() connection:Disconnect() end)
        end
    end
    antiTaserConnections = {}
end

Tab1:AddSection({
    Name = "Taser"
})

Tab1:AddToggle({
    Name = "‚ö´ Anti-Taser",
    Default = false,
    Callback = function(Value)
        AntiTaserEnabled = Value
        
        disconnectAntiTaser()
        
        if Value then
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            
            -- Prevent platform standing
            table.insert(antiTaserConnections, humanoid:GetPropertyChangedSignal("PlatformStand"):Connect(function()
                if AntiTaserEnabled and humanoid.PlatformStand then
                    humanoid.PlatformStand = false
                end
            end))
            
            -- Reset walk speed
            table.insert(antiTaserConnections, humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
                if AntiTaserEnabled and humanoid.WalkSpeed < 16 then
                    humanoid.WalkSpeed = 16
                end
            end))
            
            -- Heartbeat check
            table.insert(antiTaserConnections, RunService.Heartbeat:Connect(function()
                if AntiTaserEnabled and character:IsDescendantOf(game) then
                    if humanoid.PlatformStand then
                        humanoid.PlatformStand = false
                    end
                    if humanoid.WalkSpeed < 16 then
                        humanoid.WalkSpeed = 16
                    end
                end
            end))
            
            OrionLib:MakeNotification({
                Name = "Anti-Taser",
                Content = "Enabled",
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "Anti-Taser",
                Content = "Disabled",
                Time = 3
            })
        end
    end
})

