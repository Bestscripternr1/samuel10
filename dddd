local espEnabled = false
local boxEsp = false
local healthBarEsp = false
local nameEsp = false
local tracerEsp = false
local espColor = Color3.fromRGB(255, 0, 0)
local espTeamColor = true

local drawings = {}
local function clearDrawings()
    for _, drawing in pairs(drawings) do
        if drawing then
            drawing:Remove()
        end
    end
    drawings = {}
end

Tab4:AddToggle({
    Name = "‚ö™ ESP aktivieren",
    Default = false,
    Callback = function(Value)
        espEnabled = Value
        if not Value then
            clearDrawings()
        end
    end
})

Tab4:AddToggle({
    Name = "‚ö™ Box ESP",
    Default = false,
    Callback = function(Value)
        boxEsp = Value
    end
})

Tab4:AddToggle({
    Name = "‚ö™ Health Bar",
    Default = false,
    Callback = function(Value)
        healthBarEsp = Value
    end
})

Tab4:AddToggle({
    Name = "‚ö™ Name ESP",
    Default = false,
    Callback = function(Value)
        nameEsp = Value
    end
