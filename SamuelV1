local ReviveTab = Window:MakeTab({
    Name = "üíú‚îÇSELF REVIVE",
    Icon = "rbxassetid://7072717377",
    PremiumOnly = false
})

ReviveTab:AddParagraph("AUTO SELF-REVIVE", "Automatic hospital teleport when low health")

local autoReviveEnabled = false
local reviveThreshold = 30

ReviveTab:AddToggle({
    Name = "üíú AUTO REVIVE",
    Default = false,
    Callback = function(Value)
        autoReviveEnabled = Value
        OrionLib:MakeNotification({
            Name = Value and "‚úÖ AUTO REVIVE ON" or "‚ùå AUTO REVIVE OFF",
            Content = "Auto heal when low health",
            Time = 3
        })
    end
})

ReviveTab:AddSlider({
    Name = "‚ù§Ô∏è HEALTH THRESHOLD",
    Min = 10,
    Max = 100,
    Default = 30,
    Color = Color3.fromRGB(255, 100, 255),
    Increment = 5,
    ValueName = "HEALTH %",
    Callback = function(Value)
        reviveThreshold = Value
    end
})

spawn(function()
    while true do
        if autoReviveEnabled then
            local char = player.Character
            local humanoid = char and char:FindFirstChildOfClass("Humanoid")
            
            if humanoid and humanoid.Health <= reviveThreshold then
                instantTeleport(CFrame.new(-120.30, 10, 1077.29))
                wait(5)
            end
        end
        wait(1)
    end
end)

ReviveTab:AddButton({
    Name = "üíú HEAL NOW",
    Callback = function()
        instantTeleport(CFrame.new(-120.30, 10, 1077.29))
        OrionLib:MakeNotification({
            Name = "‚úÖ HEALING",
            Content = "Teleported to hospital",
            Time = 3
        })
    end
})

