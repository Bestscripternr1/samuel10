-- ESP TAB
local EspTab = Window:MakeTab({
    Name = "üëÅÔ∏è‚îÇESP",
    Icon = "rbxassetid://7072717077",
    PremiumOnly = false
})

EspTab:AddSection({
    Name = "üéØ VISUALS"
})

local espEnabled = false
local boxEsp = false
local healthBarEsp = false
local nameEsp = false
local tracerEsp = false
local espTeamColor = true

local drawings = {}
local function clearDrawings()
    for _, drawing in pairs(drawings) do
        if drawing then
            drawing:Remove()
        end
    end
    drawings = {}
end

EspTab:AddToggle({
    Name = "üëÅÔ∏è ESP ENABLED",
    Default = false,
    Callback = function(Value)
        espEnabled = Value
        if not Value then
            clearDrawings()
        end
    end
})

EspTab:AddToggle({
    Name = "üü¶ BOX ESP",
    Default = false,
    Callback = function(Value)
        boxEsp = Value
    end
})

EspTab:AddToggle({
    Name = "‚ù§Ô∏è HEALTH BAR",
    Default = false,
    Callback = function(Value)
        healthBarEsp = Value
    end
})

EspTab:AddToggle({
    Name = "üè∑Ô∏è NAME ESP",
    Default = false,
    Callback = function(Value)
        nameEsp = Value
    end
})

EspTab:AddToggle({
    Name = "üìè TRACER ESP",
    Default = false,
    Callback = function(Value)
        tracerEsp = Value
    end
})

EspTab:AddToggle({
    Name = "üé® TEAM COLORS",
    Default = true,
    Callback = function(Value)
        espTeamColor = Value
    end
})

local function updateESP()
    if not espEnabled then return end
    
    clearDrawings()
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local char = target.Character
            local humanoid = char:FindFirstChildOfClass("Humanoid")
            local hrp = char:FindFirstChild("HumanoidRootPart")
            
            if humanoid and hrp and humanoid.Health > 0 then
                local pos, onScreen = Camera:WorldToViewportPoint(hrp.Position)
                
                if onScreen then
                    local color = espTeamColor and target.Team and target.Team.TeamColor.Color or Color3.fromRGB(255, 50, 50)
                    
                    if boxEsp then
                        local box = Drawing.new("Square")
                        box.Visible = true
                        box.Color = color
                        box.Thickness = 2
                        box.Size = Vector2.new(100, 150)
                        box.Position = Vector2.new(pos.X - 50, pos.Y - 75)
                        box.Filled = false
                        table.insert(drawings, box)
                    end
                    
                    if healthBarEsp then
                        local healthPercent = humanoid.Health / humanoid.MaxHealth
                        local healthBar = Drawing.new("Square")
                        healthBar.Visible = true
                        healthBar.Color = Color3.new(1 - healthPercent, healthPercent, 0)
                        healthBar.Thickness = 1
                        healthBar.Size = Vector2.new(4, 100 * healthPercent)
                        healthBar.Position = Vector2.new(pos.X - 60, pos.Y - 75 + (100 * (1 - healthPercent)))
                        healthBar.Filled = true
                        table.insert(drawings, healthBar)
                    end
                    
                    if nameEsp then
                        local name = Drawing.new("Text")
                        name.Visible = true
                        name.Color = color
                        name.Size = 14
                        name.Text = target.Name
                        name.Position = Vector2.new(pos.X, pos.Y - 90)
                        name.Center = true
                        table.insert(drawings, name)
                    end
                    
                    if tracerEsp then
                        local tracer = Drawing.new("Line")
                        tracer.Visible = true
                        tracer.Color = color
                        tracer.Thickness = 1
                        tracer.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
                        tracer.To = Vector2.new(pos.X, pos.Y)
                        table.insert(drawings, tracer)
                    end
                end
            end
        end
    end
end

RunService.RenderStepped:Connect(updateESP)

EspTab:AddSection({
    Name = "üö® CRIME ESP"
})

local crimeEspEnabled = false
EspTab:AddToggle({
    Name = "üö® CRIME ESP",
    Default = false,
    Callback = function(Value)
        crimeEspEnabled = Value
    end
})

RunService.RenderStepped:Connect(function()
    if crimeEspEnabled then
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character then
                local hrp = target.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    local isWanted = hrp:GetAttribute("IsWanted")
                    if isWanted then
                        local pos, onScreen = Camera:WorldToViewportPoint(hrp.Position)
                        if onScreen then
                            local wantedText = Drawing.new("Text")
                            wantedText.Visible = true
                            wantedText.Color = Color3.fromRGB(255, 215, 0)
                            wantedText.Size = 16
                            wantedText.Text = "üö® WANTED"
                            wantedText.Position = Vector2.new(pos.X, pos.Y - 110)
                            wantedText.Center = true
                            task.spawn(function()
                                task.wait(0.1)
                                wantedText:Remove()
                            end)
                        end
                    end
                end
            end
        end
    end
end)

-- AIMBOT TAB
local AimbotTab = Window:MakeTab({
    Name = "üéØ‚îÇAIMBOT",
    Icon = "rbxassetid://7072717177",
    PremiumOnly = false
})

AimbotTab:AddSection({
    Name = "üéØ AIMBOT SETTINGS"
})

local aimbotEnabled = false
local aimbotKey = Enum.KeyCode.V
local aimbotFOV = 100
local aimbotSmoothness = 0.1
local wallbangEnabled = true
local aimbotPart = "Head"
local predictionEnabled = true
local predictionAmount = 0.14

-- FOV Circle
local fovCircle = Drawing.new("Circle")
fovCircle.Visible = false
fovCircle.Thickness = 1
fovCircle.Radius = aimbotFOV
fovCircle.Transparency = 0.5
fovCircle.Color = Color3.fromRGB(255, 50, 50)
fovCircle.Filled = false
fovCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)

AimbotTab:AddToggle({
    Name = "üéØ AIMBOT ENABLED",
    Default = false,
    Callback = function(Value)
        aimbotEnabled = Value
        fovCircle.Visible = Value
    end
})

AimbotTab:AddBind({
    Name = "üéØ AIMBOT KEY",
    Default = Enum.KeyCode.V,
    Hold = false,
    Callback = function()
        aimbotEnabled = not aimbotEnabled
        fovCircle.Visible = aimbotEnabled
        OrionLib:MakeNotification({
            Name = aimbotEnabled and "‚úÖ AIMBOT ON" or "‚ùå AIMBOT OFF",
            Content = "Hold right click to aim",
            Time = 2
        })
    end
})

AimbotTab:AddToggle({
    Name = "üß± WALLBANG ENABLED",
    Default = true,
    Callback = function(Value)
        wallbangEnabled = Value
    end
})

AimbotTab:AddDropdown({
    Name = "üéØ AIM PART",
    Default = "Head",
    Options = {"Head", "HumanoidRootPart", "Torso"},
    Callback = function(Value)
        aimbotPart = Value
    end
})

AimbotTab:AddToggle({
    Name = "üìà PREDICTION",
    Default = true,
    Callback = function(Value)
        predictionEnabled = Value
    end
})

AimbotTab:AddSlider({
    Name = "üéØ FOV SIZE",
    Min = 10,
    Max = 500,
    Default = 100,
    Color = Color3.fromRGB(255, 50, 50),
    Increment = 10,
    ValueName = "FOV",
    Callback = function(Value)
        aimbotFOV = Value
        fovCircle.Radius = Value
    end
})

AimbotTab:AddSlider({
    Name = "üîÑ SMOOTHNESS",
    Min = 0.01,
    Max = 1,
    Default = 0.1,
    Color = Color3.fromRGB(50, 255, 50),
    Increment = 0.01,
    ValueName = "SMOOTH",
    Callback = function(Value)
        aimbotSmoothness = Value
    end
})

AimbotTab:AddSlider({
    Name = "üéØ PREDICTION AMOUNT",
    Min = 0.01,
    Max = 0.5,
    Default = 0.14,
    Color = Color3.fromRGB(50, 50, 255),
    Increment = 0.01,
    ValueName = "PREDICTION",
    Callback = function(Value)
        predictionAmount = Value
    end
})

local function getClosestTarget()
    local closest = nil
    local closestDist = aimbotFOV
    local mousePos = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local char = target.Character
            local humanoid = char:FindFirstChildOfClass("Humanoid")
            local part = char:FindFirstChild(aimbotPart)
            
            if humanoid and part and humanoid.Health > 0 then
                local pos, onScreen = Camera:WorldToViewportPoint(part.Position)
                
                if onScreen or wallbangEnabled then
                    local screenPos = Vector2.new(pos.X, pos.Y)
                    local distance = (mousePos - screenPos).Magnitude
                    
                    if distance < closestDist then
                        closestDist = distance
                        closest = {target = target, part = part}
                    end
                end
            end
        end
    end
    
    return closest
end

local lastAimTime = 0
RunService.RenderStepped:Connect(function()
    fovCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
    
    if aimbotEnabled and UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) then
        if tick() - lastAimTime < 0.02 then return end
        lastAimTime = tick()
        
        local targetData = getClosestTarget()
        if targetData then
            local targetPart = targetData.part
            local targetPos = targetPart.Position
            
            if predictionEnabled then
                local targetVelocity = targetPart.Velocity
                targetPos = targetPos + (targetVelocity * predictionAmount)
            end
            
            local currentCFrame = Camera.CFrame
            local targetCFrame = CFrame.new(Camera.CFrame.Position, targetPos)
            
            Camera.CFrame = currentCFrame:Lerp(targetCFrame, aimbotSmoothness)
            
            if wallbangEnabled then
                local char = player.Character
                local tool = char and char:FindFirstChildWhichIsA("Tool")
                if tool then
                    local handle = tool:FindFirstChild("Handle")
                    if handle then
                        local args = {
                            [1] = tool,
                            [2] = targetPos,
                            [3] = (targetPos - handle.Position).Unit
                        }
                        
                        local folder = ReplicatedStorage:FindFirstChild("Bnl")
                        if folder then
                            for _, remote in pairs(folder:GetChildren()) do
                                if remote:IsA("RemoteEvent") then
                                    remote:FireServer(unpack(args))
                                    break
                                end
                            end
                        end
                    end
                end
            end
        end
    end
end)

-- POLICE TAB
local PoliceTab = Window:MakeTab({
    Name = "üëÆ‚îÇPOLICE",
    Icon = "rbxassetid://7072717277",
    PremiumOnly = false
})

PoliceTab:AddSection({
    Name = "üì° RADAR FARM"
})

local radarFarmEnabled = false
PoliceTab:AddToggle({
    Name = "üì° RADAR FARM",
    Default = false,
    Callback = function(Value)
        radarFarmEnabled = Value
        if Value then
            OrionLib:MakeNotification({
                Name = "‚úÖ RADAR FARM ON",
                Content = "Automatic speed detection",
                Time = 3
            })
        end
    end
})

spawn(function()
    while true do
        if radarFarmEnabled then
            local char = player.Character
            local radar = char and char:FindFirstChild("Radar Gun")
            local hrp = char and char:FindFirstChild("HumanoidRootPart")
            
            if radar and hrp then
                local vehicles = workspace:FindFirstChild("Vehicles")
                if vehicles then
                    for _, vehicle in pairs(vehicles:GetChildren()) do
                        local driveSeat = vehicle:FindFirstChild("DriveSeat")
                        if driveSeat and driveSeat.Occupant then
                            local folder = ReplicatedStorage:FindFirstChild("Bnl")
                            if folder then
                                local remote = folder:FindFirstChild("bbb7c252-304d-4582-b2a0-89eb9d3a0855")
                                if remote then
                                    remote:FireServer(radar, driveSeat.Position, (driveSeat.Position - hrp.Position).Unit)
                                    wait(0.5)
                                end
                            end
                        end
                    end
                end
            end
        end
        wait(1)
    end
end)

