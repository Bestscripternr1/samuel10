-- Self Revive Tab
local ReviveTab = Window:MakeTab({
    Name = "üíú‚îÇSelf Revive",
    PremiumOnly = false,
    Icon = "rbxassetid://3926305904"
})

ReviveTab:AddParagraph("Automatischer Self-Revive", "Automatisch ins Krankenhaus bei niedrigem Leben")

local autoReviveEnabled = false
local reviveThreshold = 30

ReviveTab:AddToggle({
    Name = "üíú Auto Revive",
    Default = false,
    Callback = function(Value)
        autoReviveEnabled = Value
        OrionLib:MakeNotification({
            Name = "Auto Revive",
            Content = Value and "Aktiviert" or "Deaktiviert",
            Time = 3
        })
    end
})

ReviveTab:AddSlider({
    Name = "üíú Revive Threshold",
    Min = 10,
    Max = 100,
    Default = 30,
    Color = Color3.fromRGB(255, 100, 255),
    Increment = 5,
    ValueName = "Health %",
    Callback = function(Value)
        reviveThreshold = Value
    end
})

spawn(function()
    while true do
        if autoReviveEnabled then
            local char = player.Character
            local humanoid = char and char:FindFirstChildOfClass("Humanoid")
            
            if humanoid and humanoid.Health <= reviveThreshold then
                -- Teleport to hospital
                instantTeleport(CFrame.new(-120.30, 6, 1077.29))
                
                -- Wait for healing
                wait(3)
                
                -- Find hospital bed
                local buildings = workspace:FindFirstChild("Buildings")
                if buildings then
                    local hospital = buildings:FindFirstChild("Hospital")
                    if hospital then
                        local bed = hospital:FindFirstChild("HospitalBed")
                        if bed then
                            local seat = bed:FindFirstChild("Seat")
                            if seat and humanoid then
                                seat:Sit(humanoid)
                                wait(5)
                                humanoid.Sit = false
                            end
                        end
                    end
                end
            end
        end
        wait(1)
    end
end)

ReviveTab:AddButton({
    Name = "üíú Jetzt heilen",
    Callback = function()
        local hospitalCF = CFrame.new(-120.30, 6, 1077.29)
        instantTeleport(hospitalCF)
        
        OrionLib:MakeNotification({
            Name = "Heilung",
            Content = "Zum Krankenhaus teleportiert",
            Time = 3
        })
    end
})

