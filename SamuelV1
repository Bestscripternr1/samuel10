                task.wait(0.5)
                local humanoid = char:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid:GetPropertyChangedSignal("Sit"):Connect(function()
                        if humanoid.Sit then
                            humanoid.Sit = false
