                        local hue = tick() % 5 / 5
                        local color = Color3.fromHSV(hue, 1, 1)
                        for _, part in pairs(vehicle:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.Color = color
                            end
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

