-- FPS Counter
local fpsLabel = MainTab:AddLabel("FPS: 60")
local pingLabel = MainTab:AddLabel("Ping: 0ms")
local playerCount = MainTab:AddLabel("Players: "..#Players:GetPlayers())

RunService.RenderStepped:Connect(function(delta)
    fpsLabel:Set("FPS: "..math.floor(1/delta))
end)

spawn(function()
    while wait(1) do
        pingLabel:Set("Ping: "..math.floor(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()).."ms")
        playerCount:Set("Players: "..#Players:GetPlayers())
    end
end)

