            local hrp = player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame = CFrame.new(-278.834, 7.745, 1085.797) -- Hospital
                MakeNotification("Revive", "Teleported to hospital", "rbxassetid://7072723219")
            end
        end
    end
})

