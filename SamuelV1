-- Player teleport dropdown
local playerList = {}
local playerDropdown = TeleportTab:AddDropdown({
    Name = "üë§ Select Player",
    Default = "Select Player",
    Options = {"Select Player"},
    Callback = function(selected)
        if selected ~= "Select Player" then
            local targetPlayer = Players[selected]
            if targetPlayer and targetPlayer.Character then
                local root = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
                if root then
                    instantTeleport(root.CFrame + Vector3.new(0, 5, 0))
                    OrionLib:MakeNotification({
                        Name = "Player Teleport",
                        Content = "üë§ Teleported to " .. selected,
                        Time = 2
                    })
                end
            end
        end
    end
})

-- Update player list
local function updatePlayerList()
    local options = {"Select Player"}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(options, player.Name)
        end
    end
    playerDropdown:Refresh(options, true)
end

Players.PlayerAdded:Connect(updatePlayerList)
Players.PlayerRemoving:Connect(updatePlayerList)
updatePlayerList()

-- Nearest Dealer
TeleportTab:AddButton({
    Name = "üåÄ Nearest Dealer",
    Callback = function()
        local dealers = Workspace:FindFirstChild("Dealers")
        if dealers then
            local nearest = nil
            local nearestDist = math.huge
            local charPos = LocalPlayer.Character.HumanoidRootPart.Position
            
            for _, dealer in ipairs(dealers:GetChildren()) do
                if dealer:FindFirstChild("Head") then
                    local dist = (dealer.Head.Position - charPos).Magnitude
                    if dist < nearestDist then
                        nearestDist = dist
                        nearest = dealer
                    end
                end
            end
            
            if nearest then
                instantTeleport(nearest.Head.CFrame + Vector3.new(0, 5, 0))
                OrionLib:MakeNotification({
                    Name = "Dealer Teleport",
                    Content = "üìç Teleported to nearest dealer",
                    Time = 2
                })
            end
        end
    end
})

