local rainbowVehicle = false
VehicleTab:AddToggle({
    Name = "üåà Rainbow Vehicle",
    Default = false,
    Callback = function(v)
        rainbowVehicle = v
        if v then
            spawn(function()
                while rainbowVehicle do
                    local vehicle = workspace.Vehicles:FindFirstChild(player.Name)
                    if vehicle then
                        for _, part in pairs(vehicle:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.Color = Color3.fromHSV(tick()%5/5, 1, 1)
                            end
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

-- Vehicle Teleport
VehicleTab:AddButton({
    Name = "üì¶ Teleport to Vehicle",
    Callback = function()
        local vehicle = workspace.Vehicles:FindFirstChild(player.Name)
        if vehicle and player.Character then
            local hrp = player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame = vehicle:GetPivot() * CFrame.new(0, 5, 0)
            end
        end
    end
})

