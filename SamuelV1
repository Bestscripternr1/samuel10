-- TASER TAB
local TaserTab = Window:MakeTab({
    Name = "‚ö°‚îÇTASER",
    Icon = "rbxassetid://7072716727",
    PremiumOnly = false
})

TaserTab:AddSection({
    Name = "üéØ AUTO TASER"
})

local autoTaserEnabled = false
local lastTase = 0

TaserTab:AddToggle({
    Name = "‚ö° AUTO TASER",
    Default = false,
    Callback = function(Value)
        autoTaserEnabled = Value
        OrionLib:MakeNotification({
            Name = Value and "‚úÖ AUTO TASER ON" or "‚ùå AUTO TASER OFF",
            Content = "Automatic enemy targeting",
            Time = 3
        })
    end
})

RunService.RenderStepped:Connect(function()
    if autoTaserEnabled and tick() - lastTase >= 0.5 then
        local char = player.Character
        local taser = char and char:FindFirstChild("Taser")
        if taser then
            local folder = ReplicatedStorage:FindFirstChild("Bnl")
            if folder then
                local remote = folder:FindFirstChild("c6011f40-2809-4686-a297-33283dd11715")
                if remote then
                    for _, target in pairs(Players:GetPlayers()) do
                        if target ~= player and target.Team ~= player.Team then
                            local targetChar = target.Character
                            local targetHRP = targetChar and targetChar:FindFirstChild("HumanoidRootPart")
                            if targetHRP then
                                remote:FireServer(taser, targetHRP.Position, (targetHRP.Position - (char.HumanoidRootPart.Position)).Unit)
                                break
                            end
                        end
                    end
                end
            end
        end
        lastTase = tick()
    end
end)

-- VEHICLE TAB
local VehicleTab = Window:MakeTab({
    Name = "üöó‚îÇVEHICLE",
    Icon = "rbxassetid://7072716563",
    PremiumOnly = false
})

VehicleTab:AddSection({
    Name = "üöÄ CAR FLY"
})

local carFlyEnabled = false
local carFlightSpeed = 100

local function getPlayerVehicle()
    local vehicles = workspace:FindFirstChild("Vehicles")
    if vehicles then
        return vehicles:FindFirstChild(player.Name)
    end
    return nil
end

VehicleTab:AddToggle({
    Name = "üöÄ CAR FLY",
    Default = false,
    Callback = function(Value)
        carFlyEnabled = Value
        OrionLib:MakeNotification({
            Name = Value and "‚úÖ CAR FLY ON" or "‚ùå CAR FLY OFF",
            Content = "WASD + Space/Shift to fly",
            Time = 3
        })
    end
})

VehicleTab:AddSlider({
    Name = "‚ö° FLY SPEED",
    Min = 10,
    Max = 500,
    Default = 100,
    Color = Color3.fromRGB(0, 255, 100),
    Increment = 10,
    ValueName = "SPEED",
    Callback = function(Value)
        carFlightSpeed = Value
    end
})

RunService.RenderStepped:Connect(function()
    if carFlyEnabled then
        local vehicle = getPlayerVehicle()
        local char = player.Character
        if vehicle and char then
            local driveSeat = vehicle:FindFirstChild("DriveSeat")
            if driveSeat and driveSeat.Occupant == char.Humanoid then
                if not vehicle.PrimaryPart then
                    vehicle.PrimaryPart = driveSeat
                end
                
                local moveDirection = Vector3.zero
                local camera = workspace.CurrentCamera
                
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDirection = moveDirection + camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDirection = moveDirection - camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDirection = moveDirection - camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDirection = moveDirection + camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveDirection = moveDirection + Vector3.new(0, 1, 0)
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    moveDirection = moveDirection - Vector3.new(0, 1, 0)
                end
                
                if moveDirection.Magnitude > 0 then
                    moveDirection = moveDirection.Unit * carFlightSpeed
                end
                
                local currentPos = vehicle.PrimaryPart.Position
                
                vehicle:SetPrimaryPartCFrame(
                    CFrame.new(
                        currentPos + Vector3.new(moveDirection.X, 0, moveDirection.Z) * 0.1,
                        currentPos + camera.CFrame.LookVector * 100
                    ) + Vector3.new(0, moveDirection.Y, 0)
                )
                
                driveSeat.Velocity = Vector3.zero
                driveSeat.RotVelocity = Vector3.zero
            end
        end
    end
end)

