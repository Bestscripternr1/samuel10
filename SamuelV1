        Callback = function()
            if player.Character then
                local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.CFrame = cf
                    MakeNotification("Teleport", "Teleported to "..name:sub(4), "rbxassetid://7072723219")
                end
            end
        end
    })
end

