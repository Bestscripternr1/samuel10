    Min = 1,
    Max = 10,
    Default = 2,
    Increment = 1,
    Color = Color3.fromRGB(191, 42, 42),
    ValueName = "",
    Callback = function(value)
        smoothing = (10 - value) / 10
        OrionLib:MakeNotification({
