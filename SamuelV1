    Callback = function(v) 
        if v then
            settings().Rendering.QualityLevel = 1
            game:GetService("Lighting").GlobalShadows = false
        else
            settings().Rendering.QualityLevel = 21
            game:GetService("Lighting").GlobalShadows = true
        end
    end
})

SettingsTab:AddSlider({
    Name = "üéÆ Max FPS",
    Min = 30,
    Max = 240,
    Default = 60,
    Color = Theme.Primary,
    Increment = 5,
    ValueName = "FPS",
    Callback = function(v)
        setfpscap(v)
    end
})

