-- FPS Counter
local fpsCounterEnabled = false
local fpsText
SettingsTab:AddToggle({
    Name = "üìä Show FPS",
    Default = false,
    Callback = function(v)
        fpsCounterEnabled = v
        if v then
            fpsText = Drawing.new("Text")
            fpsText.Visible = true
            fpsText.Color = Color3.new(1, 1, 1)
            fpsText.Size = 18
            fpsText.Outline = true
            fpsText.Position = Vector2.new(10, 10)
            
            spawn(function()
                while fpsCounterEnabled do
                    fpsText.Text = "FPS: " .. math.floor(1/RunService.RenderStepped:Wait())
                    task.wait(0.1)
                end
                fpsText:Remove()
            end)
        end
    end
})

