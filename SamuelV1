local player = game.Players.LocalPlayer

-- Section: Movement
PlayerTab:AddSection({
    Name = "üèÉ‚îÇMovement"
})

local noclipEnabled = false
PlayerTab:AddToggle({
    Name = "üëª Noclip",
    Default = false,
    Callback = function(v)
        noclipEnabled = v
        if v then
            game:GetService("RunService").Stepped:Connect(function()
                if noclipEnabled and player.Character then
                    for _, p in pairs(player.Character:GetDescendants()) do
                        if p:IsA("BasePart") then p.CanCollide = false end
                    end
                end
            end)
        end
    end
})

-- Fly System
local Flying = false
local FlightSpeed = 50
local FlightConnection = nil

local function ToggleFlight(enable)
    if enable then
        if not Flying then
            Flying = true
            FlightConnection = RunService.RenderStepped:Connect(function(delta)
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local root = player.Character.HumanoidRootPart
                    local direction = Vector3.new(0,0,0)
                    
                    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                        direction = direction + Camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                        direction = direction - Camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                        direction = direction - Camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                        direction = direction + Camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                        direction = direction + Vector3.new(0,1,0)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                        direction = direction - Vector3.new(0,1,0)
                    end
                    
                    if direction.Magnitude > 0 then
                        direction = direction.Unit * FlightSpeed
                    end
                    
                    root.Velocity = direction
                end
            end)
        end
    else
        Flying = false
        if FlightConnection then
            FlightConnection:Disconnect()
            FlightConnection = nil
        end
    end
end

PlayerTab:AddToggle({
    Name = "üïäÔ∏è Player Fly",
    Default = false,
    Callback = ToggleFlight
})

PlayerTab:AddSlider({
    Name = "‚úàÔ∏è Flight Speed",
    Min = 1,
    Max = 200,
    Default = 50,
    Color = Theme.Primary,
    Increment = 1,
    ValueName = "studs",
    Callback = function(v) FlightSpeed = v end
})

PlayerTab:AddBind({
    Name = "üîÑ Fly Keybind",
    Default = Enum.KeyCode.F,
    Hold = false,
    Callback = function()
        ToggleFlight(not Flying)
    end
})

-- Section: Protection
PlayerTab:AddSection({
    Name = "üõ°Ô∏è‚îÇProtection"
})

local antiTaserEnabled = false
PlayerTab:AddToggle({
    Name = "‚ö° Anti-Taser",
    Default = false,
    Callback = function(v)
        antiTaserEnabled = v
        if v and player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid:GetPropertyChangedSignal("PlatformStand"):Connect(function()
                    if antiTaserEnabled then
                        humanoid.PlatformStand = false
                    end
                end)
            end
        end
    end
})

local antiFallEnabled = false
PlayerTab:AddToggle({
    Name = "üìâ Anti-Fall Damage",
    Default = false,
    Callback = function(v)
        antiFallEnabled = v
        if v then
            RunService.Heartbeat:Connect(function()
                if antiFallEnabled and player.Character then
                    local root = player.Character:FindFirstChild("HumanoidRootPart")
                    if root and root.Velocity.Y < -50 then
                        root.Velocity = Vector3.new(root.Velocity.X, -25, root.Velocity.Z)
                    end
                end
            end)
        end
    end
})

-- Section: Tools
PlayerTab:AddSection({
    Name = "üîß‚îÇTools"
})

local deleteTool = false
PlayerTab:AddToggle({
    Name = "üóëÔ∏è Delete Tool (Click)",
    Default = false,
    Callback = function(v) deleteTool = v end
})

game:GetService("UserInputService").InputBegan:Connect(function(input)
    if deleteTool and input.UserInputType == Enum.UserInputType.MouseButton1 then
        local target = game:GetService("Players").LocalPlayer:GetMouse().Target
        if target then target:Destroy() end
    end
end)

PlayerTab:AddButton({
    Name = "üöó Exit Vehicle",
    Callback = function()
        if player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.Sit = false
                humanoid.Jump = true
            end
        end
    end
})

PlayerTab:AddButton({
    Name = "üíÄ Reset Character",
    Callback = function()
        if player.Character then
            player.Character:BreakJoints()
        end
    end
})

