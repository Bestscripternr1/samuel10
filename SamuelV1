-- Vehicle Fly System (SIMPLIFIED)
local vehicleFlying = false
local vehicleFlightSpeed = 50
local vehicleFlightConnection = nil

local function updateVehicleFlight()
    if not player.Character then return end
    
    local humanoid = player.Character:FindFirstChild("Humanoid")
    if not humanoid then return end
    
    local seatPart = humanoid.SeatPart
    if not seatPart or seatPart.Name ~= "DriveSeat" then 
        vehicleFlying = false
        if vehicleFlightConnection then
            vehicleFlightConnection:Disconnect()
            vehicleFlightConnection = nil
        end
        return 
    end
    
    local vehicle = seatPart.Parent
    if not vehicle then return end
    
    local camera = workspace.CurrentCamera
    local direction = Vector3.new(0, 0, 0)
    
    -- Movement inputs
    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
        direction = direction + camera.CFrame.LookVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
        direction = direction - camera.CFrame.LookVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
        direction = direction - camera.CFrame.RightVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
        direction = direction + camera.CFrame.RightVector
    end
    
    -- Apply speed
    if direction.Magnitude > 0 then
        direction = direction.Unit * vehicleFlightSpeed
        
        -- Move vehicle
        local currentCF = seatPart.CFrame
        local newCF = currentCF + direction * (1/60) -- Frame-based movement
        
        -- Keep vehicle upright
        local lookVector = camera.CFrame.LookVector
        lookVector = Vector3.new(lookVector.X, 0, lookVector.Z) -- Flatten
        if lookVector.Magnitude > 0 then
            lookVector = lookVector.Unit
            newCF = CFrame.new(newCF.Position, newCF.Position + lookVector)
        end
        
        vehicle:PivotTo(newCF)
        
        -- Stop natural vehicle physics
        seatPart.Velocity = Vector3.new(0, 0, 0)
        seatPart.RotVelocity = Vector3.new(0, 0, 0)
    end
end

Tab2:AddToggle({
    Name = "üü¢ Vehicle Fly",
    Default = false,
    Callback = function(Value)
        vehicleFlying = Value
        
        if vehicleFlightConnection then
            vehicleFlightConnection:Disconnect()
            vehicleFlightConnection = nil
        end
        
        if Value then
            -- Check if in vehicle
            local character = player.Character
            if character and character:FindFirstChild("Humanoid") then
                local humanoid = character.Humanoid
                if not humanoid.SeatPart then
                    OrionLib:MakeNotification({
                        Name = "Vehicle Fly Error",
                        Content = "Get in a vehicle first!",
                        Time = 3
                    })
                    return
                end
            else
                OrionLib:MakeNotification({
                    Name = "Vehicle Fly Error",
                    Content = "Get in a vehicle first!",
                    Time = 3
                })
                return
            end
            
            vehicleFlightConnection = RunService.Heartbeat:Connect(updateVehicleFlight)
            
            OrionLib:MakeNotification({
                Name = "Vehicle Fly",
                Content = "Enabled - Use WASD to move",
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "Vehicle Fly",
                Content = "Disabled",
                Time = 3
            })
        end
    end
})

Tab2:AddSlider({
    Name = "üü¢ Vehicle Fly Speed",
    Min = 1,
    Max = 100,
    Default = 50,
    Color = Color3.fromRGB(0,255,120),
    Increment = 1,
    ValueName = "Speed",
    Callback = function(Value)
        vehicleFlightSpeed = Value
    end 
})

