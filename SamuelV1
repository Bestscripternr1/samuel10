-- Player Fly (BESSER - FREIE H√ñHENKONTROLLE)
local UserCharacter = nil
local UserRootPart = nil
local Flying = false
local FlightSpeed = 50
local CurrentVelocity = Vector3.zero
local FlightConnection = nil

local function setCharacter(character)
    UserCharacter = character
    UserRootPart = character:WaitForChild("HumanoidRootPart")
end

local function Flight(delta)
    if not UserRootPart then return end
    
    local BaseVelocity = Vector3.zero
    
    if not UserInputService:GetFocusedTextBox() then
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            BaseVelocity += Camera.CFrame.LookVector * FlightSpeed
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            BaseVelocity -= Camera.CFrame.LookVector * FlightSpeed
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            BaseVelocity -= Camera.CFrame.RightVector * FlightSpeed
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            BaseVelocity += Camera.CFrame.RightVector * FlightSpeed
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            BaseVelocity += Vector3.yAxis * FlightSpeed
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
            BaseVelocity -= Vector3.yAxis * FlightSpeed
        end
    end

    if not UserRootPart.Anchored then
        UserRootPart.Velocity = BaseVelocity
        local lookVector = Camera.CFrame.LookVector
        UserRootPart.CFrame = CFrame.lookAt(UserRootPart.Position, UserRootPart.Position + Vector3.new(lookVector.X, 0, lookVector.Z))
    end
end

function ToggleFlight(enable)
    if Flying == enable then return end
    
    Flying = enable
    if enable then
        if not UserCharacter or not UserRootPart then
            local char = player.Character
            if char then
                setCharacter(char)
            end
        end
        FlightConnection = RunService.RenderStepped:Connect(Flight)
    else
        if FlightConnection then
            FlightConnection:Disconnect()
            FlightConnection = nil
        end
        if UserRootPart then
            UserRootPart.Velocity = Vector3.zero
        end
    end
end

PlayerTab:AddToggle({
    Name = "üöÄ PLAYER FLY",
    Default = false,
    Callback = function(Value)
        ToggleFlight(Value)
        OrionLib:MakeNotification({
            Name = Value and "‚úÖ FLY ON" or "‚ùå FLY OFF",
            Content = "WASD + Space/Shift to fly freely",
            Time = 3
        })
    end
})

PlayerTab:AddSlider({
    Name = "‚ö° FLY SPEED",
    Min = 10,
    Max = 200,
    Default = 50,
    Color = Color3.fromRGB(0, 200, 255),
    Increment = 5,
    ValueName = "SPEED",
    Callback = function(Value)
        FlightSpeed = Value
    end
})

