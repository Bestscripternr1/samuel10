    Name = "üõ°Ô∏è Vehicle God Mode",
    Default = false,
    Callback = function(v)
        vehicleGod = v
        if v then
            spawn(function()
                while vehicleGod do
                    local vehicle = workspace.Vehicles:FindFirstChild(player.Name)
                    if vehicle then
                        vehicle:SetAttribute("CurrentHealth", math.huge)
                        vehicle:SetAttribute("IsOn", true)
                    end
                    wait(0.5)
                end
            end)
        end
    end
})

