Tab3:AddButton({
    Name = "üîµ Zu n√§chstem Dealer",
    Callback = function()
        local dealers = workspace:FindFirstChild("Dealers")
        if dealers then
            local closest = nil
            local closestDist = math.huge
            local char = player.Character
            local hrp = char and char:FindFirstChild("HumanoidRootPart")
            
            if hrp then
                for _, dealer in pairs(dealers:GetChildren()) do
                    local head = dealer:FindFirstChild("Head")
                    if head then
                        local dist = (hrp.Position - head.Position).Magnitude
                        if dist < closestDist then
                            closestDist = dist
                            closest = head
                        end
                    end
                end
                
                if closest then
                    instantTeleport(closest.CFrame + Vector3.new(0, 5, 0))
                end
            end
        end
    end
})

