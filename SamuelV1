## SELF REVIVE TAB - PREMIUM DESIGN
-- ===========================
local ReviveTab = Window:MakeTab({ Name = "üíñ‚îÇSelf Revive", Icon = "rbxassetid://3926307971" })

ReviveTab:AddParagraph("üíñ Auto Revive System", "Automatically revives you when health is low")

-- Auto Revive
local autoReviveEnabled = false
local reviveConnection

local function checkHealthAndRevive()
    local character = LocalPlayer.Character
    if not character then return end
    
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then return end
    
    if humanoid.Health < 30 then
        -- Teleport to hospital
        local hospital = Workspace:FindFirstChild("Buildings")
        if hospital then
            hospital = hospital:FindFirstChild("Hospital")
            if hospital then
                hospital = hospital:FindFirstChild("HospitalBed")
                if hospital then
                    instantTeleport(hospital.CFrame + Vector3.new(0, 5, 0))
                    
                    task.wait(1)
                    humanoid.Health = humanoid.MaxHealth
                    
                    OrionLib:MakeNotification({
                        Name = "Auto Revive",
                        Content = "üíñ You have been revived!",
                        Time = 3
                    })
                end
            end
        end
    end
end

ReviveTab:AddToggle({
    Name = "üíñ Enable Auto Revive",
    Default = false,
    Callback = function(v)
        autoReviveEnabled = v
        if v then
            reviveConnection = RunService.Heartbeat:Connect(function()
                checkHealthAndRevive()
            end)
            OrionLib:MakeNotification({
                Name = "Auto Revive",
                Content = "üíñ Auto Revive Activated!",
                Time = 3
            })
        elseif reviveConnection then
            reviveConnection:Disconnect()
            reviveConnection = nil
        end
    end
})

-- Instant Revive Button
ReviveTab:AddButton({
    Name = "‚ö° Instant Revive",
    Callback = function()
        local character = LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.Health = humanoid.MaxHealth
                OrionLib:MakeNotification({
                    Name = "Revive",
                    Content = "‚ö° Instant Revive Complete!",
                    Time = 3
                })
            end
        end
    end
})

-- Hospital Teleport
ReviveTab:AddButton({
    Name = "üè• Teleport to Hospital",
    Callback = function()
        instantTeleport(CFrame.new(-278.834, 7.745, 1085.797))
        OrionLib:MakeNotification({
            Name = "Hospital",
            Content = "üè• Teleported to Hospital",
            Time = 2
        })
    end
})

