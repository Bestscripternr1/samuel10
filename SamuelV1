local aimbotEnabled = false
local aimbotKey = Enum.KeyCode.V
local aimbotSmoothness = 0.5
local aimbotFOV = 100
local aimPart = "Head"

AimbotTab:AddToggle({
    Name = "üî¥ Aimbot",
    Default = false,
    Callback = function(value)
        aimbotEnabled = value
    end    
})

AimbotTab:AddBind({
    Name = "üî¥ Aimbot Keybind",
    Default = Enum.KeyCode.V, 
    Hold = false,
    Callback = function()
        aimbotEnabled = not aimbotEnabled
    end    
})

AimbotTab:AddDropdown({
    Name = "üî¥ Aim Part",
    Default = "Head",  
    Options = {"Head", "Torso", "HumanoidRootPart"},  
    Callback = function(value)
        aimPart = value
    end
})

AimbotTab:AddSlider({
    Name = "üî¥ Smoothness",
    Min = 0.1,
    Max = 1,
    Default = 0.5,
    Increment = 0.1,
    Color = Color3.fromRGB(191, 42, 42),
    ValueName = "",
    Callback = function(value)
        aimbotSmoothness = value
    end
})

-- ============================
-- POLICE TAB
-- ============================
local PoliceTab = Window:MakeTab({
    Name = "üëÆ‚îÇPolice",
    PremiumOnly = false
})

PoliceTab:AddParagraph("Radar Farm","You can make with it big money")

local radarFarmEnabled = false

PoliceTab:AddToggle({
    Name = "üü§ Radar Farm",
    Default = false,
    Callback = function(Value)
        radarFarmEnabled = Value
        
        if Value then
            OrionLib:MakeNotification({
                Name = "Radar Farm",
                Content = "Started - Use radar gun",
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "Radar Farm",
                Content = "Stopped",
                Time = 3
            })
        end
    end
})

-- Anti-AFK
local antiAfkEnabled = false

PoliceTab:AddToggle({
    Name = "üü§ Anti-AFK",
    Default = false,
    Callback = function(Value)
        antiAfkEnabled = Value
        
        if Value then
            player.Idled:Connect(function()
                game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
                wait(1)
                game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
            end)
            
            OrionLib:MakeNotification({
                Name = "Anti-AFK",
                Content = "Enabled",
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "Anti-AFK",
                Content = "Disabled",
                Time = 3
            })
        end
    end
})

-- ============================
-- AUTO-REVIVE TAB
-- ============================
local ReviveTab = Window:MakeTab({
    Name = "üíâ‚îÇAuto-Revive",
    PremiumOnly = false
})

ReviveTab:AddParagraph("Auto-Revive System", "Automatically heals you at hospital")

local autoReviveEnabled = false

ReviveTab:AddToggle({
    Name = "üíâ Auto-Revive",
    Default = false,
    Callback = function(Value)
        autoReviveEnabled = Value
        
        if Value then
            spawn(function()
                while autoReviveEnabled do
                    if player.Character then
                        local humanoid = player.Character:FindFirstChild("Humanoid")
                        if humanoid and humanoid.Health < 20 then
                            OrionLib:MakeNotification({
                                Name = "Auto-Revive",
                                Content = "Low health - Healing...",
                                Time = 5
                            })
                            
                            -- Teleport to hospital
                            local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                            if hrp then
                                hrp.CFrame = CFrame.new(-278.834, 7.745, 1085.797)
                            end
                            
                            wait(5)
                            
                            -- Heal
                            if humanoid then
                                humanoid.Health = humanoid.MaxHealth
                                OrionLib:MakeNotification({
                                    Name = "Auto-Revive",
                                    Content = "Fully healed!",
                                    Time = 3
                                })
                            end
                        end
                    end
                    wait(1)
                end
            end)
            
            OrionLib:MakeNotification({
                Name = "Auto-Revive",
                Content = "Enabled - Auto-heal active",
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "Auto-Revive",
                Content = "Disabled",
                Time = 3
            })
        end
    end
})

-- ============================
-- INITIALIZE GUI
-- ============================

-- Cleanup on script end
game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
    -- Reset all movement systems
    flying = false
    vehicleFlying = false
    
    if flightConnection then
        flightConnection:Disconnect()
        flightConnection = nil
    end
    
    if vehicleFlightConnection then
        vehicleFlightConnection:Disconnect()
        vehicleFlightConnection = nil
    end
    
    -- Reset speeds
    if player.Character then
        wait(0.5)
        local humanoid = player.Character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = walkSpeedValue
            humanoid.JumpPower = jumpBoostPower
        end
    end
end)

OrionLib:Init()

OrionLib:MakeNotification({
    Name = "Script Loaded",
    Content = "Samuel v10.1 - Emergency Hamburg",
    Time = 5
})
