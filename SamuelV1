-- ===========================
-- MISC TAB (Enhanced)
-- ===========================
local MiscTab = Window:MakeTab({
    Name = "‚öôÔ∏è‚îÇMisc",
    Icon = "rbxassetid://7734074391",
    PremiumOnly = false
})

MiscTab:AddSection({
    Name = "Miscellaneous"
})

-- Anti-AFK
MiscTab:AddToggle({
    Name = "üïí Anti-AFK",
    Default = false,
    Callback = function(Value)
        Settings.AntiAFK = Value
        if Value then
            local VirtualInputManager = game:GetService("VirtualInputManager")
            spawn(function()
                while Settings.AntiAFK do
                    VirtualInputManager:SendKeyEvent(true, "A", false, game)
                    task.wait(2)
                    VirtualInputManager:SendKeyEvent(false, "A", false, game)
                    task.wait(2)
                end
            end)
        end
    end
})

-- Click TP
local clickTPEnabled = false
MiscTab:AddToggle({
    Name = "üìç Click Teleport",
    Default = false,
    Callback = function(Value)
        clickTPEnabled = Value
    end
})

mouse.Button1Down:Connect(function()
    if clickTPEnabled then
        local character = LocalPlayer.Character
        if character then
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                root.CFrame = CFrame.new(mouse.Hit.Position) + Vector3.new(0, 5, 0)
            end
        end
    end
end)

-- Infinite Yield
MiscTab:AddButton({
    Name = "‚ö° Load Infinite Yield",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
        OrionLib:MakeNotification({
            Name = "Infinite Yield",
            Content = "Loaded successfully!",
            Time = 3
        })
    end
})

-- Destroy GUI
MiscTab:AddButton({
    Name = "üóëÔ∏è Destroy GUI",
    Callback = function()
        OrionLib:Destroy()
    end
})

MiscTab:AddSection({
    Name = "Settings"
})

-- UI Toggle Key
MiscTab:AddBind({
    Name = "üéÆ UI Toggle Key",
    Default = Enum.KeyCode.RightShift,
    Hold = false,
    Callback = function()
        Window:Close()
    end
})

-- Save Settings
MiscTab:AddButton({
    Name = "üíæ Save Settings",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Settings",
            Content = "Settings saved!",
            Time = 3
        })
    end
})

