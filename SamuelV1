-- Anti Fall/Damage
local antiFallEnabled = false
local antiFallConnection = nil

PlayerTab:AddToggle({
    Name = "üõ°Ô∏è ANTI FALL/DAMAGE",
    Default = false,
    Callback = function(Value)
        antiFallEnabled = Value
        if antiFallEnabled then
            antiFallConnection = RunService.Heartbeat:Connect(function()
                local char = player.Character
                if char then
                    local humanoid = char:FindFirstChild("Humanoid")
                    local rootPart = char:FindFirstChild("HumanoidRootPart")
                    if humanoid and rootPart then
                        if humanoid:GetState() == Enum.HumanoidStateType.Freefall then
                            if rootPart.Velocity.Y < -50 then
                                rootPart.Velocity = Vector3.new(rootPart.Velocity.X, -10, rootPart.Velocity.Z)
                            end
                        end
                    end
                end
            end)
        elseif antiFallConnection then
            antiFallConnection:Disconnect()
            antiFallConnection = nil
        end
    end
})

-- Anti Taser
local AntiTaserEnabled = false
PlayerTab:AddToggle({
    Name = "‚ö° ANTI TASER",
    Default = false,
    Callback = function(Value)
        AntiTaserEnabled = Value
    end
})

game:GetService("RunService").Heartbeat:Connect(function()
    if AntiTaserEnabled and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if humanoid.PlatformStand then
                humanoid.PlatformStand = false
            end
            if humanoid:GetState() == Enum.HumanoidStateType.PlatformStanding then
                humanoid:ChangeState(Enum.HumanoidStateType.Running)
            end
        end
    end
end)

