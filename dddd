    Name = "üî¥ Aimbot Keybind",
    Default = Enum.KeyCode.V,
    Hold = false,
    Callback = function()
        aimbotEnabled = not aimbotEnabled
        fovCircle.Visible = aimbotEnabled
        OrionLib:MakeNotification({
            Name = "Aimbot",
            Content = aimbotEnabled and "Aktiviert" or "Deaktiviert",
            Time = 2
        })
    end
})

