            if noclipEffect then
                noclipEffect.Color = ColorSequence.new(Color3.fromRGB(0, 170, 255))
                noclipEffect.Size = NumberSequence.new(0.5)
                noclipEffect.Transparency = NumberSequence.new(0.7)
                noclipEffect.Lifetime = NumberRange.new(0.5)
                noclipEffect.Rate = 50
                noclipEffect.VelocitySpread = 180
            end
            
            OrionLib:MakeNotification({
                Name = "Noclip",
                Content = "‚ú® Noclip Enabled - Walk through walls",
                Image = "rbxassetid://3926307971",
                Time = 3
            })
        elseif noclipEffect then
            noclipEffect:Destroy()
        end
    end
})

-- Enhanced Fly System
local flyEnabled = false
local flySpeed = 50
local flyConnection

local function updateFly()
    if not flyEnabled or not LocalPlayer.Character then return end
    
    local humanoidRootPart = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return end
    
    local direction = Vector3.zero
    
    -- Movement controls
    if UserInputService:IsKeyDown(Enum.KeyCode.W) then direction += Camera.CFrame.LookVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.S) then direction -= Camera.CFrame.LookVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.A) then direction -= Camera.CFrame.RightVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.D) then direction += Camera.CFrame.RightVector end
    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then direction += Vector3.new(0, 1, 0) end
    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then direction -= Vector3.new(0, 1, 0) end
    
    if direction.Magnitude > 0 then
        direction = direction.Unit * flySpeed
        humanoidRootPart.Velocity = direction
    else
        humanoidRootPart.Velocity = Vector3.new(0, 0, 0)
    end
end

PlayerTab:AddToggle({
    Name = "üïäÔ∏è Player Fly",
    Default = false,
    Callback = function(v)
        flyEnabled = v
        if v then
            flyConnection = RunService.RenderStepped:Connect(updateFly)
            OrionLib:MakeNotification({
                Name = "Flight Mode",
                Content = "üïäÔ∏è Flight Activated! Controls: WASD, Space/Shift",
                Image = "rbxassetid://3926307971",
                Time = 4
            })
        elseif flyConnection then
            flyConnection:Disconnect()
            flyConnection = nil
        end
    end
})

PlayerTab:AddSlider({
    Name = "üöÄ Fly Speed",
    Min = 10,
    Max = 200,
    Default = 50,
    Color = Color3.fromRGB(0, 170, 255),
    Increment = 5,
    ValueName = "Speed",
    Callback = function(v) flySpeed = v end
})

