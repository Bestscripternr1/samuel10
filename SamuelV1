## VISUAL TAB - PREMIUM EFFECTS
-- ===========================
local VisualTab = Window:MakeTab({ Name = "‚ú®‚îÇVisual", Icon = "rbxassetid://3926305904" })

VisualTab:AddSection({ Name = "üé® Visual Effects" })

-- Night Vision
local nightVisionEnabled = false
VisualTab:AddToggle({
    Name = "üåô Night Vision",
    Default = false,
    Callback = function(v)
        nightVisionEnabled = v
        if v then
            Lighting.Ambient = Color3.new(0.5, 0.5, 0.5)
            Lighting.Brightness = 2
            Lighting.ClockTime = 0
            OrionLib:MakeNotification({
                Name = "Night Vision",
                Content = "üåô Night Vision Activated!",
                Time = 3
            })
        else
            Lighting.Ambient = Color3.new(0, 0, 0)
            Lighting.Brightness = 1
            Lighting.ClockTime = 14
        end
    end
})

-- Full Bright
local fullBrightEnabled = false
VisualTab:AddToggle({
    Name = "‚òÄÔ∏è Full Bright",
    Default = false,
    Callback = function(v)
        fullBrightEnabled = v
        if v then
            Lighting.GlobalShadows = false
            Lighting.Brightness = 3
            OrionLib:MakeNotification({
                Name = "Full Bright",
                Content = "‚òÄÔ∏è Full Bright Activated!",
                Time = 3
            })
        else
            Lighting.GlobalShadows = true
            Lighting.Brightness = 1
        end
    end
})

-- Chams (See players through walls)
local chamsEnabled = false
local chamParts = {}
VisualTab:AddToggle({
    Name = "üëÅÔ∏è Player Chams",
    Default = false,
    Callback = function(v)
        chamsEnabled = v
        if v then
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character then
                    for _, part in ipairs(player.Character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            local highlight = Instance.new("Highlight")
                            highlight.FillColor = Color3.new(1, 0, 0)
                            highlight.OutlineColor = Color3.new(1, 1, 0)
                            highlight.FillTransparency = 0.5
                            highlight.OutlineTransparency = 0
                            highlight.Parent = part
                            chamParts[part] = highlight
                        end
                    end
                end
            end
        else
            for part, highlight in pairs(chamParts) do
                highlight:Destroy()
            end
            chamParts = {}
        end
    end
})

