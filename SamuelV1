-- ============================
-- FUNCTIONS
-- ============================
-- Auto-Revive System
UtilityTab:AddSection({
    Name = "üè•‚îÇAuto-Revive"
})

local autoRevive = false
UtilityTab:AddToggle({
    Name = "üíâ Auto-Revive",
    Default = false,
    Callback = function(v)
        autoRevive = v
        if v then
            spawn(function()
                while autoRevive do
                    if player.Character then
                        local humanoid = player.Character:FindFirstChild("Humanoid")
                        if humanoid and humanoid.Health < 20 then
                            -- Teleport to hospital logic
                            MakeNotification("Auto-Revive", "Healing at hospital...", "rbxassetid://7072723219")
                            wait(5)
                            if humanoid then
                                humanoid.Health = humanoid.MaxHealth
                            end
                        end
                    end
                    wait(1)
                end
            end)
        end
    end
})

-- Speed Boost
PlayerTab:AddSection({
    Name = "‚ö°‚îÇSpeed"
})

local speedBoost = false
local walkSpeed = 16
PlayerTab:AddToggle({
    Name = "‚ö° Speed Boost",
    Default = false,
    Callback = function(v)
        speedBoost = v
        if v and player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 50
            end
        elseif player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 16
            end
        end
    end
})

PlayerTab:AddSlider({
    Name = "üö∂ Walk Speed",
    Min = 16,
    Max = 100,
    Default = 16,
    Color = Theme.Primary,
    Increment = 1,
    ValueName = "studs",
    Callback = function(v)
        walkSpeed = v
        if player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid and not speedBoost then
                humanoid.WalkSpeed = v
            end
        end
    end
})

-- Jump Power
local jumpPower = 50
PlayerTab:AddSlider({
    Name = "ü¶ò Jump Power",
    Min = 50,
    Max = 200,
    Default = 50,
    Color = Theme.Primary,
    Increment = 5,
    ValueName = "power",
    Callback = function(v)
        jumpPower = v
        if player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.JumpPower = v
            end
        end
    end
})

-- ============================
-- INITIALIZATION
-- ============================
-- Load saved settings
spawn(function()
    wait(1)
    MakeNotification("Welcome!", "Emergency Hamburg Premium v10.1 loaded!", "rbxassetid://4483345998")
end)

-- Auto-update player stats
spawn(function()
    while true do
        if player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                if not speedBoost then
                    humanoid.WalkSpeed = walkSpeed
                end
                humanoid.JumpPower = jumpPower
            end
        end
        wait(0.5)
    end
end)

-- Initialize GUI
OrionLib:Init()

