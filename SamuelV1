                local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.CFrame = cf
                    MakeNotification("Teleport", "Teleported to "..name, "rbxassetid://7072723219")
                end
            end
        end
    })
end

