-- Rainbow Vehicle
local rainbowVehicle = false
VehicleTab:AddToggle({
    Name = "üåà Rainbow Vehicle",
    Default = false,
    Callback = function(Value)
        rainbowVehicle = Value
        if Value then
            spawn(function()
                while rainbowVehicle do
                    local vehiclesFolder = Workspace:FindFirstChild("Vehicles")
                    if vehiclesFolder then
                        local playerVehicle = vehiclesFolder:FindFirstChild(LocalPlayer.Name)
                        if playerVehicle then
                            for _, part in pairs(playerVehicle:GetDescendants()) do
                                if part:IsA("BasePart") then
                                    part.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                                end
                            end
                        end
                    end
                    task.wait(0.1)
                end
            end)
        end
    end
})

