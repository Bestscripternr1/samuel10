local PlayerTab = Window:MakeTab({
    Name = "üë§‚îÇPlayer",
    Icon = "rbxassetid://7072717835",
    PremiumOnly = false
})

-- Section: Movement
PlayerTab:AddSection({
    Name = "üèÉ‚îÇMovement"
})

local player = game.Players.LocalPlayer

-- Noclip
local noclipEnabled = false
PlayerTab:AddToggle({
    Name = "üëª Noclip",
    Default = false,
    Flag = "Noclip",
    Callback = function(v)
        noclipEnabled = v
        if v then
            game:GetService("RunService").Stepped:Connect(function()
                if noclipEnabled and player.Character then
                    for _, p in pairs(player.Character:GetDescendants()) do
                        if p:IsA("BasePart") then p.CanCollide = false end
                    end
                end
            end)
        end
    end
})

-- Fly System
local Flying = false
local FlightSpeed = 50
local FlightConnection = nil

local function ToggleFlight(enable)
    if enable then
        if not Flying then
            Flying = true
            FlightConnection = RunService.RenderStepped:Connect(function(delta)
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local root = player.Character.HumanoidRootPart
                    local direction = Vector3.new(0,0,0)
                    
                    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                        direction = direction + Camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                        direction = direction - Camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                        direction = direction - Camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                        direction = direction + Camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                        direction = direction + Vector3.new(0,1,0)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                        direction = direction - Vector3.new(0,1,0)
                    end
                    
                    if direction.Magnitude > 0 then
                        direction = direction.Unit * FlightSpeed
                    end
                    
                    root.Velocity = direction
                end
            end)
        end
    else
        Flying = false
        if FlightConnection then
            FlightConnection:Disconnect()
            FlightConnection = nil
        end
    end
end

PlayerTab:AddToggle({
    Name = "üïäÔ∏è Player Fly",
    Default = false,
    Flag = "PlayerFly",
    Callback = ToggleFlight
})

PlayerTab:AddSlider({
    Name = "‚ö° Flight Speed",
    Min = 1,
    Max = 200,
    Default = 50,
    Color = Theme.Primary,
    Increment = 1,
    ValueName = "studs",
    Flag = "FlightSpeed",
    Callback = function(v) FlightSpeed = v end
})

PlayerTab:AddBind({
    Name = "üîë Fly Keybind",
    Default = Enum.KeyCode.F,
    Hold = false,
    Flag = "FlyKeybind",
    Callback = function()
        ToggleFlight(not Flying)
    end
})

