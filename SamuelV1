-- Funktion f√ºr Notifications mit Icon
local function MakeNotification(title, content, icon, color)
    OrionLib:MakeNotification({
        Name = title,
        Content = content,
        Image = icon or "rbxassetid://4483345998",
        Time = 5
    })
end

